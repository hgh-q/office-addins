                        <bt:Image size="80" resid="Icon.16x16" />
                      </Icon>
                    	<Action xsi:type="ExecuteFunction">
                        <FunctionName>btnconnectservice</FunctionName>
                      </Action>
                    </Item>
                  </Items>
                </Control>
							</Group>
              <Label resid="TabCustom1.customTab.Label" />
